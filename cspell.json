{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"ignorePaths": ["pnpm-lock.yaml", "**.svg", ".git/", ".gitignore", ".vscode/", "./patches/"],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"codeowner",
		"moneytor",
		"multitenancy",
		"pawelblaszczyk",
		"stylesheet",
		"tokenami",
		"typecheck",
		"unopinionated"
	],
	"enableGlobDot": true,
	"enableFiletypes": ["dockerfile", "github-actions-workflow"],
	"ignoreWords": [
		"commonality",
		"commonalityco",
		"isbot",
		"knip",
		"nuqs",
		"pipeable",
		"predev",
		"replicache",
		"taze",
		"tseslint",
		"xyflow"
	]
}
