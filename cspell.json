{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"ignorePaths": ["pnpm-lock.yaml", "**.svg", ".git/", ".gitignore", ".vscode/", "./patches/"],
	"dictionaries": ["lorem-ipsum"],
	"useGitignore": true,
	"words": [
		"isbot",
		"moneytor",
		"multitenancy",
		"pipeable",
		"replicache",
		"taze",
		"tseslint",
		"typecheck",
		"unopinionated"
	],
	"enableGlobDot": true,
	"enableFiletypes": ["dockerfile", "github-actions-workflow"]
}
