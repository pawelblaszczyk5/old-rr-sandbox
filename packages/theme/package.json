{
	"name": "@moneytor/theme",
	"version": "0.0.1",
	"private": true,
	"description": "Stylex variables for theming",
	"repository": {
		"type": "git",
		"url": "https://github.com/pawelblaszczyk5/moneytor",
		"directory": "packages/theme"
	},
	"sideEffects": false,
	"type": "module",
	"imports": {
		"#src/*": "./build/src/*"
	},
	"exports": {
		"./colors.stylex": "./build/src/colors.stylex.js"
	},
	"scripts": {
		"build": "tsc --noCheck",
		"dev": "tsc --noCheck",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"typecheck": "tsc --build --noEmit"
	},
	"dependencies": {
		"@moneytor/stylex": "workspace:0.0.1"
	},
	"devDependencies": {
		"@moneytor/eslint-config": "workspace:0.0.1",
		"@moneytor/typescript-config": "workspace:0.0.1",
		"typescript": "catalog:"
	}
}
