{
	"name": "@moneytor/css",
	"version": "0.0.1",
	"private": true,
	"description": "Shared code for Tokenami usage",
	"repository": {
		"type": "git",
		"url": "https://github.com/pawelblaszczyk5/moneytor",
		"directory": "packages/css"
	},
	"sideEffects": false,
	"type": "module",
	"imports": {
