	},
	"dependencies": {
		"@tokenami/css": "catalog:",
		"@tokenami/dev": "catalog:",
		"@tokenami/ds": "catalog:"
	},
	"devDependencies": {
		"@moneytor/eslint-config": "workspace:0.0.1",
		"@moneytor/typescript-config": "workspace:0.0.1",
		"typescript": "catalog:"
	}
